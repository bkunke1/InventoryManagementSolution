{
  "httpsPort": 5488,
  "certificate": {
    "key": "certificates/server.key",
    "cert": "certificates/server.cert"
  },
  "store": {
    "provider": "memory"
  },
  "blobStorage": {
    "provider": "memory"
  },
  "logger": {
    "console": {
      "transport": "console",
      "level": "debug"
    }
  },
  "allowLocalFilesAccess": true,
  "reportTimeout": 60000,
  "templatingEngines": {
    "strategy": "http-server"
  },
  "extensions": {
    "authentication": {
      "cookieSession": {},
      "admin": {
        "username": "admin",
        "password": "password"
      },
      "enabled": false
    },
    "scripts": {
      "strategy": "http-server"
    }
  }
}